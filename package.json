{
  "name": "ashswap-monitor",
  "version": "0.0.1",
  "description": "Ashswap monitor",
  "author": "Bicarus Lab",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "prepare": "husky install",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:devnet": "npm run copy-devnet-beta-config & nest start",
    "start:devnet:watch": "npm run copy-devnet-beta-config & nest start --watch",
    "start:devnet:debug": "npm run copy-devnet-beta-config & nest start --watch --debug",
    "start:test:devnet": "npm run copy-devnet-alpha-config & nest start",
    "start:test:devnet:watch": "npm run copy-devnet-alpha-config & nest start --watch",
    "start:test:devnet:debug": "npm run copy-devnet-alpha-config & nest start --watch --debug",
    "start:testnet": "npm run copy-testnet-config & nest start",
    "start:testnet:watch": "npm run copy-testnet-config & nest start --watch",
    "start:testnet:debug": "npm run copy-testnet-config & nest start --watch --debug",
    "start:mainnet": "npm run copy-mainnet-config & nest start",
    "start:custom": "npm run copy-custom-config & nest start",
    "start:custom:watch": "npm run copy-custom-config & nest start --watch",
    "start:custom:debug": "npm run copy-custom-config & nest start --watch --debug",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "copy-devnet-beta-config": "cp ./pool_config/pool.devnet.beta.yaml ./pool_config/pool.yaml",
    "copy-devnet-alpha-config": "cp ./config/config.devnet.alpha.yaml ./config/config.yaml",
    "copy-testnet-config": "cp ./config/config.testnet.yaml ./config/config.yaml",
    "copy-mainnet-config": "cp ./pool_config/pool.mainnet.yaml ./pool_config/pool.yaml",
    "copy-custom-config": "cp ./config/config.custom.yaml ./config/config.yaml"
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^3.5.0",
    "@multiversx/sdk-core": "^11.4.0",
    "@multiversx/sdk-nestjs": "^0.4.8",
    "@multiversx/sdk-network-providers": "^1.3.0",
    "@multiversx/sdk-transaction-decoder": "^1.0.2",
    "@multiversx/sdk-transaction-processor": "^0.1.29",
    "@nestjs/apollo": "^10.2.0",
    "@nestjs/bull": "^0.6.3",
    "@nestjs/cli": "^9.2.0",
    "@nestjs/common": "^9.1.4",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.9",
    "@nestjs/graphql": "^10.2.0",
    "@nestjs/microservices": "^9.3.9",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/platform-socket.io": "^9.3.9",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.3.9",
    "@ntegral/nestjs-sentry": "^4.0.0",
    "@sentry/browser": "^7.38.0",
    "@trancport/aggregator": "0.2.2",
    "agentkeepalive": "^4.2.1",
    "bignumber.js": "^9.1.1",
    "body-parser": "^1.20.2",
    "bull": "^4.10.4",
    "cache-manager": "^5.1.7",
    "cookie-parser": "^1.4.6",
    "graphql": "^16.6.0",
    "graphql-fields-list": "^2.2.4",
    "graphql-request": "^6.0.0",
    "husky": "^8.0.3",
    "ioredis": "^5.3.1",
    "jsonwebtoken": "^9.0.0",
    "nest-winston": "^1.8.0",
    "pg": "^8.9.0",
    "prom-client": "^14.1.1",
    "rxjs": "^7.8.0",
    "socket.io": "^4.6.1",
    "store2": "^2.14.2",
    "typeorm": "^0.3.12",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/bignumber.js": "^5.0.0",
    "@types/bull": "^3.15.9",
    "@types/cache-manager": "^4.0.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/ioredis": "^4.28.10",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "js-yaml": "^4.1.0",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "{src,apps,libs,test}/**/*.ts": [
      "prettier --write"
    ]
  }
}
